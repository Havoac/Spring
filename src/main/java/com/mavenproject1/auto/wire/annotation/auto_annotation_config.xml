<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context.xsd">

<!--uses type autowiring-->
<context:annotation-config />

<!-- uses autowire:byType --> 
<bean class="com.mavenproject1.auto.wire.annotation.Address" name="address1" >
	<property name="street" value="ADA" />
	<property name="city" value="Prayagraj" />
</bean>

<bean class="com.mavenproject1.auto.wire.annotation.Address" name="address2" >
	<property name="street" value="ADA" />
	<property name="city" value="Delhi" />
</bean>

<bean class="com.mavenproject1.auto.wire.annotation.Emp" name="emp1" />

</beans>