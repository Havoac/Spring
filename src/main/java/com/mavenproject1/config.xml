<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd">

<!--this is our beans-->

<!--this student1 is an object name-->
<bean class="com.mavenproject1.Student" name="student1">
	<property name="studentId" value="1" />	
	
	<property name="studentName">
		<value>Shivansh Singh</value>
	</property>	
	
	<property name="studentAddress">
		<value>Noida</value>
	</property>	
</bean>

<bean class="com.mavenproject1.Student" name="student2" p:studentId="2" p:studentName="Shivam" p:studentAddress="Lucknow" />

</beans>