<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd">

<bean class="com.mavenproject1.collections.Emp" name="emp1">
	<property name="name" value="Aman"></property>
	
	<!--Collection type injection-->
	<property name="phones">
		<list>
			<value>1234</value>
			<value>5678</value>
			<value>1234</value>
			<null></null>
		</list>
	</property>
	
	<property name="address">
		<set>
			<value> Delhi</value>	
			<value>Kolkata</value>		
		</set>
	</property>
	
	<property name="courses">
		<map>
			<entry key="java" value="2 month" />
			<entry key="c++" value="1 month" />
		</map>
	</property>
	
	<property name="props">
		<props>
			<prop key="name">Shivansh</prop>
			<prop key="designation">Game developer</prop>
		</props>		
	</property>
</bean>

</beans>